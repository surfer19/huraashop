<h1>
  ORDER CHECKOUT!
 </h1>

 {{#each this.model.baskets as |product|}}
   <p>
    <strong>Name:</strong> {{product.title}}
    <strong>Price/unit:</strong> £{{product.price}}
    <strong>Discount:</strong> £{{product.applyDiscount}}
    <strong>Quantity:</strong> {{product.quantity}}
   </p>
   {{!-- <ProductList @onConfirm={{action "addedItemToBasket"}} @product={{productUnit}} /> --}}
 {{/each}}
  <p>All discounts applied: {{this.model.discount}}</p>
  <p>Total price after discount: {{this.model.totalPriceAfterDiscount}}</p>
